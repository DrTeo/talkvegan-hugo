{
    "data" : {{ block "response" .}}{{ end }},
    "languages": [
      {{- range  $index, $element :=  $.Site.Languages }}
        {{ if $index}}, {{ end }}
        {
          "shortCode": {{.Lang | jsonify }},
          "name": {{.LanguageName | jsonify }}
        }
      {{ end }}
    ],
    "languageName": {{.Site.Language.LanguageName | jsonify}},
    "languageShortCode": {{.Site.Language.Lang | jsonify}},
    "date": {{ now  | jsonify }}
}
